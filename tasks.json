[
  {
    "label": "Run Project",
    "command": "sh",
    "args": [
      "~/.config/zed/runners/custom_runproject.sh",
      "file=$ZED_FILE",
      "fileName=$ZED_FILENAME",
      "fileNameWithoutExt=$ZED_STEM",
      "dirName=$ZED_DIRNAME",
      "relativeFile=$ZED_RELATIVE_FILE",
      "stem=$ZED_STEM",
      "workspaceRoot=$ZED_WORKTREE_ROOT"
    ],
    "shell": {
      "program": "/bin/zsh"
    },
    "show_summary": false,
    "show_command": false,
    "show_output": false
  },
  {
    "label": "Run File",
    "command": "sh",
    "args": [
      "~/.config/zed/runners/custom_runfile.sh",
      "file=$ZED_FILE",
      "fileName=$ZED_FILENAME",
      "fileNameWithoutExt=$ZED_STEM",
      "dirName=$ZED_DIRNAME",
      "relativeFile=$ZED_RELATIVE_FILE",
      "stem=$ZED_STEM",
      "workspaceRoot=$ZED_WORKTREE_ROOT"
    ],
    "shell": {
      "program": "/bin/zsh"
    },
    "show_summary": false,
    "show_command": false,
    "show_output": false
  },
  {
    "label": "Npm Dev",
    "command": "npm run $script",
    "env": { "script": "dev" },
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "never",
    "shell": "system",
    "show_summary": false,
    "show_command": false,
    "show_output": false
  },
  {
    "label": "Npm Build",
    "command": "npm run $script",
    "env": { "script": "build" },
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "never",
